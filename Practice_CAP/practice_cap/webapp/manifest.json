{
  "_version": "1.65.0",
  "sap.app": {
    "id": "app.practicecap",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.1",
      "toolsId": "bee6cbe6-387a-4be1-bf80-4a660450f76b"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.129.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "app.practicecap.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "app.practicecap.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Login",
          "pattern": ":?query:",
          "target": [
            "Login"
          ]
        },
        {
          "name": "Dashboard",
          "pattern": "dashboard",
          "target": [
            "Dashboard"
          ]
        }
        
      ],
      "targets": {
        "Login": {
          "viewType": "XML",
          "viewId": "Login",
          "viewName": "Login"
        },
        "Dashboard": {
          "viewType": "XML",
          "viewId": "Dashboard",
          "viewName": "Dashboard"
        },
        "Sample": {
          "viewType": "XML",
          "viewId": "Sample",
          "viewName": "view.Sample"
        }
      }
    },
    "rootView": {
      "viewName": "app.practicecap.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
